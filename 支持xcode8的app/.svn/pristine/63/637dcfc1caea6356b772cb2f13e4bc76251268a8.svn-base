#import <Foundation/Foundation.h>

@interface EvaModel : NSObject
/*
 eva_list：List<Map>
 
 user：评价用户名称1
 user_img：评价用户头像1
 content：评价内容1
 addTime：评价时间1
 evaluate_photos：晒单（可没有）1
 add_content：追加评价（可没有）
 add_evaluate_photos：追加晒单（可没有）
 */
@property (nonatomic, copy)NSString *user;
@property (nonatomic, copy)NSString *user_img;
@property (nonatomic, copy)NSString *content;
@property (nonatomic, copy)NSString *addTime;
@property (nonatomic, copy)NSString *evaluate_photos;
@property (nonatomic, copy)NSString *add_content;
@property (nonatomic, copy)NSString *add_evaluate_photos;

+(instancetype)evaModelWithDict:(NSDictionary *)dict;

@end
